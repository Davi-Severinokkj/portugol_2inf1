programa {
  funcao inicio() {
    inteiro n1, antecessor, sucessor
    
    escreva("Digite um número: ")
    leia(n1)

    antecessor = n1 - 1
    sucessor = n1 + 1

    // Use a vírgula para separar as variáveis e os textos
    escreva("O antecessor é ", antecessor, " e o seu sucessor é ", sucessor)
  }
}
