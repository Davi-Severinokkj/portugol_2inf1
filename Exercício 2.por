programa {
  funcao inicio() {
    real n1, n2, sub, mult, div
    escreva("Digite um número: ")
    leia(n1)
    escreva("Digite outro número: ")
    leia(n2)

    sub = n1 - n2
    mult = n1 * n2
    div = n1 / n2

    escreva("A subtração é igual a " + sub + ", a multiplicação é igual a " + mult + " e a divisão é igual a " + div +".")
  }
}
